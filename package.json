    "q": "^1.2.0",
    "ractive": "^0.6.1",
    "ractive-validator": "^2.0.8",
    "winston": "^0.9.0"
  },
  "browser": {
    "ractive": "./node_modules/ractive/ractive.min.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "loose": true,
