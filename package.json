{
  "name": "itlenergy-web-client",
  "version": "0.1.0",
  "scripts": {
    "build": "grunt"
  },
  "dependencies": {
    "babel": "^4.7.12",
    "babel-runtime": "^4.7.12",
    "bluebird": "^2.9.14",
    "bootstrap": "^3.3.2",
    "commander": "^2.7.1",
    "compression": "^1.4.2",
    "cubism": "^1.6.0",
    "d3": "^3.5.5",
    "director": "^1.2.8",
    "express": "git://github.com/stewartml/express",
    "font-awesome": "^4.3.0",
    "hogan-express": "^0.5.2",
    "jquery": "^1.11.2",
