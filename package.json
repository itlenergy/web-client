  },
  "browserify-shim": {
    "jquery": "jQuery",
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.5.0",
    "grunt-contrib-clean": "^0.6.0",
